pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                script {
                    checkout([$class: 'GitSCM',
                                branches: [[name: branchName]],
                                doGenerateSubmoduleConfigurations: false,
                                extensions: [],
                                submoduleCfg: [],
                                userRemoteConfigs: [[url: 'https://github.com/Bhumika-gowda/jenkins_pipeline_jobs_may24.git',
                                                    credentialsId: 'github_bg']]])
                }
            }
        }
    }
}
